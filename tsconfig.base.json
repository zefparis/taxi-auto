{
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "target": "es2020",
    "lib": ["es2020"],
    "composite": true,
    "types": ["node"]
  },
  "exclude": ["node_modules"]
}
